#include<stdio.h>
int main(void)
{
 int a,b,c,d;
 printf("enter the value");
 scanf("%d%d",&a,&b);
 printf(" enter choice 1.add\n2.sub\n3.mul\n4.div\n5.mod\n");
 scanf("%d",&d);
 switch(d)
 {
 case 1:
 {
  c=a+b;
  printf("%d is add",c);
  break;
  }
  case 2:
  {
  c=a-b;
  printf("%d is sub",c);
  break;
  }
  case 3:
  {
  c=a*b;
  printf("%d is mul",c);
  break;
  }
  case 4:
  {
  c=a/b;
  printf("%d is div",c);
  break;
  }
  case 5:
  {
  c=a%b;
  printf("%d is mod",c);
  break;
  }
  default:
  {
  printf("valid options");
  break;
  }
  }
  return 0;
  }
