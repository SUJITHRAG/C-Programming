#include <stdio.h>
#include<string.h>
int main(void)
{
 int i,a;
 char str[10],temp;
 gets(str);
 strlen(str);
 if(a%2==0)
 {
 for(i=0;i<strlen(str);i=i+2)
 {
 	temp=str[i];
 	str[i]=str[i+1];
 	str[i+1]=temp;
          
 }
 }
 else
 {
  for(i=0;i=strlen(str)-1;i=i+2)
  {
  	temp=str[i];
 	str[i]=str[i+1];
 	str[i+1]=temp;
  }
 }
 for(i=0;i<strlen(str);i++)
 {
 	printf("%c",str[i]);
 }
 
 return 0;
}
